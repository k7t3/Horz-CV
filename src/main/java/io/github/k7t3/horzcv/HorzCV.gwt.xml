<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="horzcv">
    <!--<module>-->

    <!-- modules -->
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="gwt.material.design.GwtMaterialBasicWithJQuery"/>
    <inherits name="gwt.material.design.themes.ThemeBlue"/>
    <inherits name="io.github.k7t3.horzcv.client.Resources"/>

    <!-- logging -->
    <set-property name="gwt.logging.logLevel" value="INFO"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <!--  <set-property name="gwt.logging.consoleHandler" value="DISABLED"/>-->

    <!-- locale -->
    <extend-property name="locale" values="ja,en"/>
    <set-property name="locale" value="ja,en"/>
    <set-property-fallback name="locale" value="en" />

    <source path="client" />

    <entry-point class="io.github.k7t3.horzcv.client.App" />

    <!-- Only support recent browsers -->
    <set-property name="user.agent" value="gecko1_8,safari" />

</module>
