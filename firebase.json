{
  "hosting": {
    "public": "horzcv-client/build/gwt/js",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "**/api/**",
        "function": {
          "functionId": "api",
          "region": "us-central1"
        }
      },

      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}